<p style="color: green"><%= notice %></p>
<div style="text-align: center">
  <h1 class="header-text">Latest questions</h1>
</div>

<div id="questions">
  <% @questions.each do |question| %>
    <%= render question %>
    <hr>
  <% end %>
</div>
<div class="apple_pagination">
  <%= will_paginate @questions, :container => false %>
</div>